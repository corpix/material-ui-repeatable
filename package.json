{
  "name": "material-ui-repeatable",
  "version": "1.0.0",
  "description": "Repeat any component as many times as you like on the page.",
  "main": "Repeatable.js",
  "scripts": {
    "build": "babel src -d lib",
    "prepublish": "babel src -d ./ && cd src && find . -type f -name '*.css' | xargs -I{} cp {} ../{}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corpix/material-ui-repeatable.git"
  },
  "keywords": [
    "react",
    "material",
    "repeat",
    "copy",
    "dom"
  ],
  "author": "Dmitry Moskowski <me+material-ui-repeatable@corpix.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corpix/material-ui-repeatable/issues"
  },
  "homepage": "https://github.com/corpix/material-ui-repeatable#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "material-ui": "^0.16.2",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-tap-event-plugin": "^2.0.0"
  },
  "peerDependencies": {
    "material-ui": ">= 0.15.0 <= 0.16.x",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-tap-event-plugin": "^1.0.0 || ^2.0.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.2"
  }
}
