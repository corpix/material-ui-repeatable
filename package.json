    "type": "git",
    "url": "git+https://github.com/corpix/material-ui-repeatable.git"
  },
  "keywords": [
    "react",
    "material",
    "repeat",
    "copy",
    "dom"
  ],
  "author": "Dmitry Moskowski <me+material-ui-repeatable@corpix.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corpix/material-ui-repeatable/issues"
  },
  "homepage": "https://github.com/corpix/material-ui-repeatable#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "material-ui": "^0.18.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-tap-event-plugin": "^2.0.1"
  },
  "dependencies": {
